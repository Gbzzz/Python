import random

lista_valores = list(range(26))
num_escolhido = []
num_escolhido.append(lista_valores[random.randint(0,26)])
tentativa = int(input(">>Digite um número: "))
dica_maior = tentativa < num_escolhido[0]
dica_menor = tentativa > num_escolhido[0]
print(num_escolhido)

i = 0
for i in range(5):
   if tentativa == num_escolhido[0]:
       print("Parabéns, vc acertou")
       break
   i += 1
   if i == 5:
       break
   if tentativa != num_escolhido[0]:
       print("ERROOOOUUU")
       tentativa = int(input(">>Digite um número: "))
